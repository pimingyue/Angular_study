<!--pages/classic/classic.wxml-->

<view class='container'>
  <view class='header'>
    <v-epsoide class='epsoide' index="{{classic.index}}" />
    <v-like class='like' bind:like="onLike" like='{{likeStatus}}' count='{{likeCount}}' />
  </view>





  <v-movie img="{{classic.images}}" content="{{classic.content}}" />

  <v-navi class='navi' title='{{classic.title}}' latest='{{latest}}' first='{{first}}' bind:left="onNext" bind:right="onPrevious"  />

</view>

